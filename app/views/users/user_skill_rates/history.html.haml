.container
  %h1
    %span #{user.skills_history_header} skills history
    - if user == current_user
      = link_to 'Skill rating', user_skill_rates_path, class: 'btn btn-xs btn-default'
  = react_component('user_skill_history',
    { user_id: user.id,
    skill_categories: skill_categories,
    api_token: AppConfig.api_token })
